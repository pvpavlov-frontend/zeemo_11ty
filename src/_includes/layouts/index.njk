            <h2>Contact form</h2>
            <div class="contact-form">
              <form method="POST" name="user-data" data-netlify-recaptcha="true" data-netlify="true">
                <div class="contact-form__layout-container">
                  <div class="contact-form__buffer-container">
                    <label class="contact-form__label" for="user-name">
                      Enter your name
                    </label>
                  </div>
                  <div class="contact-form__buffer-container">
                    <input class="contact-form__field" type="text" name="user-name" id="user-name" placeholder="Sergei Dragunov" required>
                  </div>
                </div>
                <div class="contact-form__layout-container">
                  <div class="contact-form__buffer-container">
                    <label class="contact-form__label" for="user-email">
                      Enter your email
                  </label>
                  </div>
                  <div class="contact-form__buffer-container">
                    <input class="contact-form__field" type="email" name="user-name" id="user-email" placeholder="example@gmail.com" required>
                  </div>
                </div>
                <div class="contact-form__layout-container">
                  <div class="contact-form__buffer-container">
                    <label class="contact-form__label" for="user-phone">
                        Enter your phone
                    </label>
                  </div>
                  <div class="contact-form__buffer-container">
                    <input class="contact-form__field" type="tel" name="user-phone" id="user-phone" placeholder="(555) 555 1234" required>
                  </div>
                </div>
                <div class="contact-form__layout-container">
                  <div class="contact-form__buffer-container">
                    <label class="contact-form__label" for="user-message">
                      Enter your message
                  </label>
                  </div>
                  <div class="contact-form__buffer-container">
                    <textarea class="contact-form__field" name="user-message" id="user-message" placeholder="What you are want to tell us" required></textarea>
                  </div>
                </div>
                <div class="contact-form__buffer-container">
                    <div class="g-recaptcha" id="recaptcha" data-sitekey="6Lfcs48aAAAAANzQ4HtzkIeHe5mpe6UPBFFpUcne">
                    </div>
                    <span><b>There should be google recaptcha</b></span>
                </div>
                <div class="contact-form__buffer-container">
                  <button class="contact-form__submit" type="submit">submit</button>
                </div>
              </form>
            </div>

