---
permalink: "assets/css/typography.css"
eleventyExcludeFromCollections: "true"
---

body {
  margin: <%= typography.elements.body['margin'] %>;
  padding: <%= typography.elements.body['padding'] %>;
  font-family: <%= typography.elements.body['font-family'] %>;
}

.container {
  max-width: <%= typography.elements.container['max-width'] %>;
  padding: <%= typography.elements.container['padding'] %>;
  margin: <%= typography.elements.container['margin'] %>;
}

.dimensional-container {
  padding: <%= typography.elements['dimensional-container']['padding'] %>;
}

