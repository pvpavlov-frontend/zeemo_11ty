<div class="section contact-us">
  <div class="container">
    <div class="spacer-50"></div>
    <div class="spacer-20"></div>
    <div class="spacer-20"></div>
    <section>
      <div class="contact-us__inner">
        <div class="blocks-container blocks-container--wrap blocks-container--2 clearfix">
          <div class="blocks-container__item">
            <div class="dimensional-container">
              <div class="contact-form">
                <div class="blocks-container__item-buffer-outer">
                  <form method="POST">
                    <div class="contact-form__layout-container">
                      <div class="contact-form__buffer-container">
                        <label class="contact-form__label" for="user-name">
                          Name
                        </label>
                      </div>
                      <div class="contact-form__buffer-container">
                        <input class="contact-form__field" type="text" name="user-name" id="user-name"
                          placeholder="Your Name" required>
                      </div>
                    </div>
                    <div class="blocks-container blocks-container--nowrap blocks-container--2 clearfix">
                      <div class="blocks-container__item">
                        <div class="contact-form__layout-container">
                          <div class="contact-form__buffer-container">
                            <label class="contact-form__label" for="user-email">
                              Email
                            </label>
                          </div>
                          <div class="contact-form__buffer-container">
                            <input class="contact-form__field" type="email" name="user-email" id="user-email"
                              placeholder="Your Email" required>
                          </div>
                        </div>
                      </div>
                      <div class="blocks-container__item">
                        <div class="contact-form__layout-container">
                          <div class="contact-form__buffer-container">
                            <label class="contact-form__label" for="user-phone">
                              Telephone
                            </label>
                          </div>
                          <div class="contact-form__buffer-container">
                            <input class="contact-form__field" type="tel" name="user-phone" id="user-phone"
                              placeholder="Your Phone" required>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="contact-form__layout-container">
                      <div class="contact-form__buffer-container">
                        <label class="contact-form__label" for="user-message">
                          Additional Details
                        </label>
                      </div>
                      <div class="contact-form__buffer-container">
                        <textarea class="contact-form__field" name="user-message" id="user-message"
                          placeholder="Additional Details" required></textarea>
                      </div>
                    </div>
                    <div class="contact-form__buffer-container">
                      <button class="contact-form__submit primary-button" type="submit">Submit Request</button>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>
          <div class="blocks-container__item">
            <div class="blocks-container__item-buffer-outer">
              <div class="blocks-container__item-buffer-inner">
                <h2 class="h2--color">
                  Let's Get In Touch
                </h2>
                <address>
                  <div class="blocks-container blocks-container--nowrap blocks-container--2 clearfix">
                    <div class="blocks-container__item">
                      <div class="address-block">
                        <h3 class="h4">
                          <i aria-hidden="true" class="elementkit-infobox-icon icon icon-map-marker1"></i>
                          <b>
                            Address
                          </b>
                        </h3>
                        <p>
                          Breslauer Allee 48, 31787 Hameln, Germany
                        </p>
                      </div>
                    </div>
                    <div class="blocks-container__item">
                      <div class="address-block">
                        <h3 class="h4">
                          <i aria-hidden="true" class="elementkit-infobox-icon icon icon-envelope2"></i>
                          <b>
                            Email
                          </b>
                        </h3>
                        <p>
                          Email: hello@zeemo.app
                        </p>
                      </div>
                    </div>
                  </div>
                </address>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <div class="spacer-50"></div>
    <div class="spacer-20"></div>
    <div class="spacer-20"></div>
  </div>
</div>
